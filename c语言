#include<stdio.h>
int main()
{
	int a=0;
	int b=0;
	scanf("%d %d",&a,&b);
	int c=0;
	if(a>b)
	{
		for(c=a;c>=b;c++)
		{
			if(c%b==0&&c%a==0)
			{
			printf("%d",c);
			break;
			}
			
		}
	}
	else
	{
		for (c=b;c>=a;c++)
		{
			if(c%b==0&&c%a==0)
			{
				printf("%d",c);
				break;
			}
		}
	}
	
	return 0;
}
